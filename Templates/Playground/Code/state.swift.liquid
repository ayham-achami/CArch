import CArch

/// Протокол передающий доступ к некоторым свойствам состояние модуля `{{ module_info.name }}` как только для чтения
protocol {{ module_info.name }}ModuleReadOnlyState: AnyReadOnlyState {}

/// Протокол передающий доступ к состоянию модуля как только для чтения
protocol {{ module_info.name }}ModuleStateRepresentable: AnyModuleStateRepresentable {
    
    var readOnly: {{ module_info.name }}ModuleReadOnlyState { get }
}

/// Состояние модуля `{{ module_info.name }}`
struct {{ module_info.name }}ModuleState: ModuleState {    
    
    struct InitialState: ModuleInitialState {}
    
    struct FinalState: ModuleFinalState {}
    
    typealias InitialStateType = InitialState
    typealias FinalStateType = FinalState
    
    var initialState: {{ module_info.name }}ModuleState.InitialStateType?
    var finalState: {{ module_info.name }}ModuleState.FinalStateType?
}

// MARK: - {{ module_info.name }}ModuleState +  ReadOnly
extension {{ module_info.name }}ModuleState: {{ module_info.name }}ModuleReadOnlyState {}